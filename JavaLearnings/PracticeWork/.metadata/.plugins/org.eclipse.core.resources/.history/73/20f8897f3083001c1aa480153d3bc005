package ObjectOrientedProgramming;

public class Student {

	public static void main(String[] args) {
		
		//States
		String name;
		int age;
		String phone;
		char sex;
	}
		
		//Behaviours - method
		public void eat(){
			System.out.println("Eating");
		}
		
		public void drink(){
			System.out.println("Drinking");
		}
		
		public void run(){
			System.out.println("Running");
		}
		

}
