	public static void main(String[] args) {
		
		private int a = 12;
		//How to get data 
		//We use getters and setters methods for accessing and modifying the values of private variables
		/*
		 * public String getName(){
		 * 		return name;
		 * }
		 * 
		 * public int getAge(){
		 * 		return age;
		 * }
		 */

	}

}
