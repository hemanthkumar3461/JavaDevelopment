package Arraylist;
//ArrayList implements list interface
//Many prefer arrayList than arrays
//because it is a resizable/dynamic implementation using list interface
//ArrayList can dynamically shrink when elements are added/removed from list


public class ArrayDeclarations {

	int[] arr1 = {1,2,3};
	
	int[] arr2 = new int[10];
	
	int[] arr3 = new int[] {1,2,3};
	
  }