package com.school.project;

public class Student {		
		private int id;
		private String name;
		private int grade;
		private int feespaid;
		private int feestotal;
		
		public Student(int id, String name, int grade) {
			feespaid = 0;
			feestotal=30000;
			
			
			this.id = id;
			this.name = name;
			this.grade = grade;
		}
		
		public void setGrade(int grade) {
			this.grade = grade;
			
		}
		
		public void updateFeesPaid(int fees) {
			feespaid+=fees;
			
		}
		
		
		
		
		
		
		
		

	

}
